<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ez.wonder.skill.model.SkillDAO">
	<select id="allLanguage" resultType="languageVo">
		select * from language
		order by lang_No asc
	</select>

	<select id="allFrame" resultType="frameworkVo">
		select* from framework 
		order by frame_No asc
	</select>


</mapper>